{
	"version": 2,
	"builds": [
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "dist"
			}
		},
		{
			"src": "api/chat/index.py",
			"use": "@vercel/python"
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "/api/chat/index.py"
		},
		{
			"src": "/assets/(.*)",
			"headers": {
				"cache-control": "s-maxage=31536000,immutable"
			},
			"dest": "/assets/$1"
		},
		{
			"src": "/(.*\\..*)",
			"dest": "/$1"
		},
		{
			"src": "/(.*)",
			"dest": "/index.html"
		}
	]
}
