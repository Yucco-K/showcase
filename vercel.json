{
	"builds": [
		{
			"src": "api/chat/index.py",
			"use": "@vercel/python"
		},
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "dist"
			}
		}
	],
	"rewrites": [
		{
			"source": "/gorse-api/api/:path*",
			"destination": "http://18.183.35.86:8087/api/:path*"
		},
		{
			"source": "/api/:path*",
			"destination": "/api/chat/index.py"
		},
		{
			"source": "/:path*",
			"destination": "/index.html"
		}
	]
}
