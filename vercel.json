{
	"builds": [
		{
			"src": "api/chat/index.py",
			"use": "@vercel/python"
		},
		{
			"src": "package.json",
			"use": "@vercel/static-build",
			"config": {
				"distDir": "dist"
			}
		}
	],
	"routes": [
		{
			"src": "/api/(.*)",
			"dest": "/api/chat/index.py"
		},
		{
			"src": "/assets/(.*)",
			"dest": "/assets/$1"
		},
		{
			"handle": "filesystem"
		},
		{
			"src": "/(.*)",
			"dest": "/index.html"
		}
	]
}
